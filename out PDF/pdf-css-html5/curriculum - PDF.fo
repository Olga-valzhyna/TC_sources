                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Just the Docs
                                        gives your documentation a jumpstart with a responsive
                                        Jekyll theme that is easily customizable and hosted on
                                        GitHub Pages.</fo:block></fo:block-container>
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_13_Connect_42_section_i3y_mr3_ppb" chfox:nd-id="section_i3y_mr3_ppb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d36e4112" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Just
                                            the Docs</fo:block></fo:block-container>
